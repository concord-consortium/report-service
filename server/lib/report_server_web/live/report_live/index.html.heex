<.header>
  Your Reports
</.header>

<.async_result :let={aws_data} assign={@aws_data}>
  <:loading>Loading your reports...</:loading>
  <:failed :let={{:error, reason}}>There was an error loading your reports: <%= reason %></:failed>
  Workgroup: <%= aws_data.workgroup["workgroupName"] %>
</.async_result>



